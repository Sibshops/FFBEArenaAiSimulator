Esper Esper::CreateSiren()
{
  Esper esper;

  esper.m_Name = "Siren";
  esper.HP = 3275;
  esper.MP = 3445;
  esper.ATK = 1835;
  esper.DEF = 1980;
  esper.MAG = 3655;
  esper.SPR = 3655;

  esper.Abilities.push_back(&SongOfAttack);
  esper.Abilities.push_back(&AlluringAir);
  esper.Abilities.push_back(&Deshell);
  esper.Abilities.push_back(&Lullaby);
  esper.Abilities.push_back(&);
  esper.Abilities.push_back(&);
  esper.Abilities.push_back(&);
  esper.Abilities.push_back(&);
  esper.Abilities.push_back(&);
	    
  
  return esper;
}


