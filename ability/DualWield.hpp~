#include "../stats/BattleStats.hpp"
#include "Ability.hpp"

#ifndef SWORD_MASTER_HPP
#define SWORD_MASTER_HPP

class SwordMaster : public Ability
{
public:
  
  bool IsStats() const override { return true;} ;

  void ModifyStats(BattleStats& bs) override
  {
    bs.ATK += bs.BaseATK * 1.2;
  };
  
};

#endif
